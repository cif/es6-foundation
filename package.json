{
  "name": "es6-foundation",
  "version": "1.0.0",
  "description": "Just in case the world needed more ES6 dev boilerplate...",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register --reporter dot src/**/*.test.js",
    "dev": "node webpack.js",
    "start": "node express.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cif/es6-foundation.git"
  },
  "keywords": [
    "boilerplate",
    "react",
    "redux",
    "eslint",
    "mocha",
    "teaspoon"
  ],
  "author": "Ben Ipsen <email@benipsen.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/cif/es6-foundation/issues"
  },
  "homepage": "https://github.com/cif/es6-foundation#readme",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "~7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "~6.4.3",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "~12.0.0",
    "eslint-plugin-import": "~1.16.0",
    "eslint-plugin-jsx-a11y": "~2.2.3",
    "eslint-plugin-react": "~6.4.0",
    "isparta": "~4.0.0",
    "jsdom": "^9.8.0",
    "mocha": "~2.3.4",
    "proxyquire": "^1.7.10",
    "react-hot-loader": "^3.0.0-beta.6",
    "sinon": "^1.17.6",
    "teaspoon": "~6.4.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.0"
  },
  "dependencies": {
    "babel-eslint": "7.0.0",
    "express": "^4.14.0",
    "firebase": "^3.5.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-actions": "^0.12.0",
    "redux-thunk": "^2.1.0"
  },
  "babel": {
    "plugins": [
      "transform-object-rest-spread",
      [
        "transform-runtime",
        {
          "polyfill": true,
          "regenerator": true
        }
      ],
      "transform-react-jsx"
    ],
    "presets": [
      "es2015",
      "react"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "experimentalObjectRestSpread": true,
      "sourceType": "module",
      "modules": true
    },
    "env": {
      "mocha": true
    },
    "rules": {
      "comma-dangle": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-named-as-default": 0,
      "import/no-unresolved": 0,
      "import/extensions": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/no-multi-comp": [
        2,
        {
          "ignoreStateless": true
        }
      ],
      "react/forbid-prop-types": [
        1,
        {
          "forbid": [
            "any"
          ]
        }
      ],
      "react/no-unused-prop-types": [
        1,
        {
          "skipShapeProps": true
        }
      ],
      "new-cap": [
        2,
        {
          "capIsNewExceptions": [
            "List",
            "Map"
          ]
        }
      ]
    }
  }
}
